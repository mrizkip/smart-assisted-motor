<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hanyasoftware.android.smartassistedmotor">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".SAMApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity" />
        <activity android:name=".motor.MotorActivity" />
        <activity android:name=".diagnosis.DiagnosisActivity" />
        <activity android:name=".bengkel.BengkelActivity" />
        <activity android:name=".riwayat.RiwayatActivity" />
        <activity android:name=".guest.GuestActivity" />
        <activity android:name=".pengaturan.PengaturanActivity" />
        <activity android:name=".bengkel.DetailBengkelActivity" />
        <activity android:name=".guest.DetailGuestActivity" />
        <activity android:name=".pengaturan.PengaturanAlarmActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".main.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pengaturan.PengaturanSuaraActivity" />
        <activity android:name=".pengaturan.TambahMotorActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".pilihmotor.PilihMotorActivity"></activity>
    </application>

</manifest>